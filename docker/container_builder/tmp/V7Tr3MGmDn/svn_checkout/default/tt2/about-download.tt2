[%-
    META
       content_type        = "application/pdf"
       content_disposition = 'attachment; filename="About.pdf"'
-%]

[%-
    style = 'just_content'
-%]

[%-
    url = conf.webserver_info.fe_url _ '/page-as-summary.tt2?SD_KEY=' _ args.SD_KEY _ '&page_name=' _ args.page_name;
    conf.url_to_pdf(url);
-%]
